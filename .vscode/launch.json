{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome Extension",
			"runtimeArgs": [
				"--load-extension=${workspaceFolder}/dist",
				"--user-data-dir=${workspaceFolder}/.vscode-chrome-debug"
			],
			"runtimeExecutable": "stable",
			"webRoot": "${workspaceFolder}",
			"file": "${workspaceFolder}/test/index.html"
		}
	]
}
